<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.bidabehere.DS.finaltp10">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".Estudiante"
            android:label="@string/app_name"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <!--
 ATTENTION: This was auto-generated to add Google Play services to your project for
     App Indexing.  See https://g.co/AppIndexing/AndroidStudio for more information.
        -->
        <meta-data
            android:name="com.google.android.gms.version"
            android:value="@integer/google_play_services_version" />

        <activity
            android:name=".Estudiante.Registro"
            android:label="@string/title_activity_registro"
            android:parentActivityName=".Estudiante">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.example.juan.finaltp10.Estudiante" />
        </activity>
        <activity android:name=".Estudiante.activity_registro" />
        <activity
            android:name=".Estudiante.registrar"
            android:label="@string/title_activity_registrar" />
        <activity android:name=".Estudiante.noticias" />
        <activity android:name=".Estudiante.MainActivity"></activity>
    </application>

</manifest>